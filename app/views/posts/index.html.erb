<% if user_signed_in? %>
 	<p>Welcome <%= current_user.username %></p>
<% else %>
	<%= render("layouts/jumbotron") %>\
<% end %>



<% @posts.each do |post| %>
	
	<%= link_to (image_tag post.image.url(:medium)), post %>
	<h4><%= link_to post.title, post %></h4>
	<p><%= link_to " #{post.user.username}", post.user %></p>

<% end %>